<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="com.Ecole.Controller.EtudiantsController">
   <children>
      <TableView fx:id="afficher_etudiant" layoutY="81.0" prefHeight="273.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="afficher_etudiant_id" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="afficher_etudiant_nom" prefWidth="75.0" text="Nom" />
            <TableColumn fx:id="afficher_etudiant_prenom" prefWidth="75.0" text="Prenom" />
            <TableColumn fx:id="afficher_etudiant_email" prefWidth="75.0" text="Email" />
            <TableColumn fx:id="afficher_etudiant_classe" prefWidth="75.0" text="Classe" />
        </columns>
      </TableView>
      <ComboBox fx:id="classes" layoutY="360.0" prefWidth="150.0" promptText="Classes" />
      <Button fx:id="affecter" layoutX="179.0" layoutY="360.0" mnemonicParsing="false" onAction="#AffecterEtudiant" text="Affecter" />
      <Button fx:id="gestionclasses" layoutX="49.0" layoutY="28.0" mnemonicParsing="false" onAction="#GererClasses" text="Gestion des Classes" />
      <Button fx:id="gestionbranches" layoutX="196.0" layoutY="28.0" mnemonicParsing="false" onAction="#GererBranches" text="Gestion des branches" />
   </children>
</AnchorPane>
