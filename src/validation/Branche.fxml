<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="459.0" prefWidth="732.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Ecole.Controller.BrancheController">
   <children>
      <Button fx:id="gestion_etudiants" layoutX="160.0" layoutY="25.0" mnemonicParsing="false" onAction="#GestionEtudiants" text="Gestion des etudiants" />
      <TabPane layoutY="100.0" prefHeight="359.0" prefWidth="732.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Ajout Branches">
            <content>
              <AnchorPane fx:id="tabBranche" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextField fx:id="nombranche" layoutX="49.0" layoutY="108.0" promptText="Nom" />
                        <Button fx:id="ajoutBranche" layoutX="61.0" layoutY="168.0" mnemonicParsing="false" onAction="#ajoutBranche" text="Ajouter une branche" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Gerer Branches">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="afficher_branche" prefHeight="261.0" prefWidth="732.0">
                          <columns>
                            <TableColumn fx:id="afficher_branche_id" prefWidth="281.60003054142" text="ID" />
                            <TableColumn fx:id="afficher_branche_nom" onEditCommit="#editNom" prefWidth="449.5999755859375" text="Nom" />
                          </columns>
                        </TableView>
                        <Button fx:id="delete_branche" layoutX="301.0" layoutY="273.0" mnemonicParsing="false" onAction="#DeleteBranche" text="Supprimer la branche" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <Button fx:id="gestion_classes" layoutY="25.0" mnemonicParsing="false" onAction="#GestionClasses" text="Gestion des classes" />
   </children>
</AnchorPane>
